! State:           'bg',     'fg',     'bgalt',  'hlbg',   'hlfg'
rofi.color-normal: #282A36,  #F8F8F2,  #282A36,  #44475A,  #5AF78E
rofi.color-urgent: #282A36,  #dc322f,  #4D4D4D,  #dc322f,  #282A36
rofi.color-active: #282A36,  #268bd2,  #4D4D4D,  #268bd2,  #282A36

!                  'background', 'border', 'separator'
rofi.color-window: #282A36,      #282A36,  #F4F99D
rofi.separator-style: solid
